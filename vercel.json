{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/bfhl",
      "dest": "/api/bfhl"
    },
    {
      "src": "/bfhl/(.*)",
      "dest": "/api/bfhl"
    }
  ],
  "functions": {
    "api/bfhl.js": {
      "maxDuration": 10
    }
  }
}
